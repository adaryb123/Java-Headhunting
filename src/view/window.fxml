<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.ColorAdjust?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>


<AnchorPane xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ControllerWindow">
   <children>
      <BorderPane fx:id="BorderPaneContent" prefHeight="700.0" prefWidth="1000.0" style="-fx-background-color: #5500ff;">
         <top>
            <HBox fx:id="HBoxHeader" alignment="CENTER_LEFT" prefHeight="30.0" prefWidth="1000.0" spacing="20.0">
               <children>
                  <Button fx:id="ButtonHome" mnemonicParsing="false" onAction="#ButtonHomeOnAction" text="Home" />
               </children>
               <BorderPane.margin>
                  <Insets />
               </BorderPane.margin>
               <effect>
                  <ColorAdjust />
               </effect>
            </HBox>
         </top>
      </BorderPane>
   </children>
</AnchorPane>
